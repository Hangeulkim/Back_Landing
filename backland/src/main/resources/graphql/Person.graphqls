type Query{
    getPeople(request: SearchRequest): [PhoneResponse]
}

type Mutation{
    createOrUpdatePerson(request: PhoneRequest) : PhoneResponse!
    deletePerson(request: DeleteRequest!): Float
}

type PhoneResponse{
    name: String
    phone: String
}

input SearchRequest{
    name: String
    phone: String
}

input PhoneRequest{
    name: String!
    phone: String!
}

input DeleteRequest{
    phone: String!
}